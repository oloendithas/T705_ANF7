<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Alert" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="MonthView_WeekLabel">
        <item name="android:textSize">@dimen/month_week_text_size</item>
        <item name="android:textColor">@color/month_week_w</item>
        <item name="android:gravity">top|center</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginTop">@dimen/month_week_margin_top</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="MonthView_WeekNameLabel">
        <item name="android:textSize">@dimen/month_week_text_size</item>
        <item name="android:textColor">@color/month_week_name</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@color/week_label_cell_color</item>
        <item name="android:paddingLeft">@dimen/month_week_label_padding_left</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="TextAppearance" parent="@android:style/TextAppearance" />
    <style name="MonthView_border_horizontal">
        <item name="android:background">@color/month_line</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/month_line_width</item>
    </style>
    <style name="MonthView_border_vertical">
        <item name="android:background">@color/month_line</item>
        <item name="android:layout_width">0.5dip</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="widget_label">
        <item name="android:textSize">@dimen/widget_label_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/widget_label</item>
        <item name="android:gravity">left|center</item>
        <item name="android:layout_width">@dimen/widget_label_width</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginLeft">@dimen/widget_label_margin_left</item>
    </style>
    <style name="header_line">
        <item name="android:background">@drawable/widget_top_bg_line</item>
        <item name="android:layout_width">1.0dip</item>
        <item name="android:layout_height">44.0dip</item>
        <item name="android:layout_marginTop">0.0dip</item>
    </style>
    <style name="header_today">
        <item name="android:textSize">@dimen/widget_label_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/widget_label</item>
        <item name="android:background">@drawable/widgetheader_today_btn_selector</item>
        <item name="android:layout_width">@dimen/header_today_width</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:text">@string/today_theme</item>
        <item name="android:contentDescription">@string/accessibility_today_button</item>
    </style>
    <style name="header_btn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/header_btn_height</item>
        <item name="android:layout_marginTop">0.0dip</item>
    </style>
    <style name="header_refresh" parent="@style/header_btn">
        <item name="android:background">@drawable/widgetheader_refresh_btn_selector</item>
        <item name="android:layout_width">@dimen/refresh_btn_width</item>
        <item name="android:layout_marginRight">@dimen/month_refresh_btn_margin_right</item>
        <item name="android:src">@drawable/widget_planner_ic_refresh</item>
        <item name="android:contentDescription">@string/accessibility_refresh_button</item>
    </style>
    <style name="header_add" parent="@style/header_btn">
        <item name="android:background">@drawable/widgetheader_add_btn_selector</item>
        <item name="android:layout_width">@dimen/add_btn_width</item>
        <item name="android:src">@drawable/widget_planner_ic_add</item>
        <item name="android:contentDescription">@string/accessibility_create_event_button</item>
    </style>
    <style name="header_handwriting" parent="@style/header_btn">
        <item name="android:background">@drawable/widgetheader_add_btn_selector</item>
        <item name="android:layout_width">@dimen/handwriting_btn_width</item>
        <item name="android:src">@drawable/widget_planner_ic_handwriting</item>
        <item name="android:contentDescription">@string/menu_handwriting_mode_on</item>
    </style>
    <style name="header_arrow_left">
        <item name="android:background">@drawable/widgetheader_arrow_left_btn_selector</item>
        <item name="android:focusable">false</item>
        <item name="android:layout_width">@dimen/widgetheader_arrow_btn_width</item>
        <item name="android:layout_height">@dimen/widgetheader_arrow_btn_height</item>
        <item name="android:layout_marginTop">@dimen/widgetheader_arrow_btn_margin_top</item>
        <item name="android:contentDescription">@string/accessibility_previous_day_button</item>
    </style>
    <style name="header_arrow_right">
        <item name="android:background">@drawable/widgetheader_arrow_right_btn_selector</item>
        <item name="android:focusable">false</item>
        <item name="android:layout_width">@dimen/widgetheader_arrow_btn_width</item>
        <item name="android:layout_height">@dimen/widgetheader_arrow_btn_height</item>
        <item name="android:layout_marginTop">@dimen/widgetheader_arrow_btn_margin_top</item>
        <item name="android:layout_marginRight">@dimen/widgetheader_arrow_btn_margin_right</item>
        <item name="android:contentDescription">@string/accessibility_next_day_button</item>
    </style>
    <style name="month_move_button">
        <item name="android:textSize">@dimen/month_move_btn_text_size</item>
        <item name="android:textColor">@color/month_title_color</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">@dimen/month_move_btn_width</item>
        <item name="android:layout_height">@dimen/month_move_btn_height</item>
        <item name="android:layout_marginTop">@dimen/month_move_btn_margin_top</item>
    </style>
    <style name="month_move_button_container">
        <item name="android:background">@drawable/widgetheader_button_selector</item>
        <item name="android:focusable">true</item>
        <item name="android:layout_width">@dimen/month_move_btn_container_width</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="today_move_button_container">
        <item name="android:background">@drawable/widgetheader_button_selector</item>
        <item name="android:focusable">true</item>
        <item name="android:layout_width">@dimen/today_move_btn_container_width</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="month_move_button_text">
        <item name="android:textSize">@dimen/month_move_btn_text_size</item>
        <item name="android:textColor">@color/month_title_color</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="WidgetDayOfWeekStyle">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">-5.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="WidgetDayStyle">
        <item name="android:textSize">24.0sp</item>
        <item name="android:gravity">bottom|left|center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/widget_day_num_top_padding</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="WidgetMonthStyle">
        <item name="android:textSize">24.0sp</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">-4.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="month_date_container">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/month_title_height</item>
    </style>
    <style name="day_item_time_style">
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="monthview_style">
        <item name="android:background">@drawable/widget_planner_bg_01_bottom</item>
        <item name="android:layout_marginLeft">0.0dip</item>
    </style>
    <style name="EasyWidgetDateStyle">
        <item name="android:textSize">@dimen/appwidget_easy_header_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/month_title_color_easy</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/appwidget_easy_header_text_margintop</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowRadius">3.0</item>
    </style>
    <style name="widget_header_line_new">
        <item name="android:background">@color/widget_header_line_color</item>
        <item name="android:layout_width">@dimen/widget_header_line_width</item>
        <item name="android:layout_height">@dimen/widget_header_line_height</item>
        <item name="android:layout_marginTop">@dimen/widget_header_line_margin_top</item>
    </style>
    <style name="mg_label_light">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#fff5f5f5</item>
        <item name="android:gravity">left|center</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="mg_label_regular">
        <item name="android:textSize">@dimen/mg_calendar_item</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#fff5f5f5</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="mg_label_header">
        <item name="android:textSize">21.0dip</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#fff5f5f5</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="magazine_month_title_style" parent="@style/mg_label_header">
        <item name="android:textSize">@dimen/magazine_month_title</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">14.0dip</item>
        <item name="android:layout_marginRight">14.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">#80000000</item>
        <item name="android:shadowDx">-1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">2.0</item>
    </style>
    <style name="magazine_header_btn_style">
        <item name="android:textSize">8.0sp</item>
        <item name="android:gravity">right</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="magazine_num_events_style" parent="@style/mg_label_light">
        <item name="android:textSize">@dimen/magazine_num_events</item>
        <item name="android:gravity">left|center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">24.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="magazine_widget_btn_style">
        <item name="android:textSize">@dimen/widget_label_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/widget_label</item>
        <item name="android:background">@drawable/magazine_week_divider</item>
        <item name="android:layout_width">@dimen/header_today_width</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:text">@string/today_theme</item>
        <item name="android:contentDescription">@string/accessibility_today_button</item>
    </style>
    <style name="mg_week_day_label_style" parent="@style/mg_label_light">
        <item name="android:textSize">@dimen/mg_week_day</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">@dimen/mg_week_day_label_width</item>
        <item name="android:layout_height">@dimen/mg_week_day_label_height</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="magazine_no_event_layout_style">
        <item name="android:textSize">21.0dip</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#fff5f5f5</item>
        <item name="android:gravity">center</item>
        <item name="android:shadowColor">#40000000</item>
        <item name="android:shadowDx">-1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">2.0</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
</resources>
