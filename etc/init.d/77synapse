#!/system/bin/sh

echo 1900000 > /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq  # 2100 max
echo 100000 > /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq

echo 2 > /sys/devices/system/cpu/sched_mc_power_savings

echo 0 > /sys/block/mmcblk0/queue/add_random
echo 0 > /sys/block/mmcblk1/queue/add_random

echo 0 > /sys/block/mmcblk0/queue/nomerges
echo 0 > /sys/block/mmcblk1/queue/nomerges

echo 2 > /sys/block/mmcblk0/queue/rq_affinity
echo 2 > /sys/block/mmcblk1/queue/rq_affinity

echo 100 > /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq_gpu
echo 480 > /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq_gpu


echo 0 > /sys/class/misc/wolfson_control/switch_eq_speaker
echo 0 > /sys/class/misc/wolfson_control/switch_eq_headphone

# /sys/class/misc/wolfson_control/eq_hp_freqs
# 0x0fc8 0x03fe 0x00e0 0x1ec4 0xf136 0x0409 0x04cc 0x1c9b 0xf337 0x040b 0x0cbb 0x16f8 0xf7d9 0x040a 0x1f14 0x058c 0x0563 0x4000 

# /sys/class/misc/wolfson_control/eq_sp_freqs
# 0x0fc8 0x03fe 0x00e0 0x1ec4 0xf136 0x0409 0x04cc 0x1c9b 0xf337 0x040b 0x0cbb 0x16f8 0xf7d9 0x040a 0x1f14 0x058c 0x0563 0x4000 





